<UserControl x:Class="PrismMultipleRegionApp.Views.ViewB"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:PrismMultipleRegionApp.Views"
             xmlns:Prism="http://prismlibrary.com/"
             Prism:RegionManager.RegionManager="{Binding CurrentRegionManager}"
             Prism:ViewModelLocator.AutoWireViewModel="True"
             mc:Ignorable="d"
             d:DesignHeight="300"
             d:DesignWidth="300">
    <Grid>
        <ContentControl Prism:RegionManager.RegionName="ChildMain" />
    </Grid>
</UserControl>
